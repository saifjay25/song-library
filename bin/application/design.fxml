<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="713.0" prefWidth="358.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller">
   <children>
      <ListView fx:id="songList" layoutY="180.0" onMouseClicked="#listSelected" prefHeight="400.0" prefWidth="358.0" />
      <Button fx:id="add" layoutX="233.0" layoutY="34.0" mnemonicParsing="false" onAction="#addClicked" text="Add" />
      <TextField fx:id="artist" layoutX="14.0" layoutY="14.0" promptText="Enter Artist" />
      <TextField fx:id="song" layoutX="14.0" layoutY="47.0" promptText="Enter Song" />
      <TextField fx:id="album" layoutX="14.0" layoutY="82.0" promptText="Enter Album (optional)" />
      <TextField fx:id="year" layoutX="14.0" layoutY="118.0" promptText="Enter Year (optional)" />
      <Text fx:id="popup" layoutX="15.0" layoutY="167.0" strokeType="OUTSIDE" strokeWidth="0.0" />
      <Text fx:id="sart" layoutX="97.0" layoutY="606.0" strokeType="OUTSIDE" strokeWidth="0.0" />
      <Text fx:id="ssong" layoutX="97.0" layoutY="633.0" strokeType="OUTSIDE" strokeWidth="0.0" />
      <Text fx:id="salbum" layoutX="97.0" layoutY="658.0" strokeType="OUTSIDE" strokeWidth="0.0" />
      <Text fx:id="syear" layoutX="96.0" layoutY="681.0" strokeType="OUTSIDE" strokeWidth="0.0" />
      <Text layoutX="14.0" layoutY="606.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Artist" />
      <Text layoutX="14.0" layoutY="633.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Song" />
      <Text layoutX="12.0" layoutY="658.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Album" />
      <Text layoutX="16.0" layoutY="681.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Year" />
      <Button fx:id="delete" layoutX="226.0" layoutY="127.0" mnemonicParsing="false" onAction="#deleteClicked" text="Delete" />
      <Button fx:id="edit" layoutX="233.0" layoutY="82.0" mnemonicParsing="false" onAction="#editClicked" text="Edit" />
   </children>
</AnchorPane>
